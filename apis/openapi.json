                            "description": "Printer Y dimension in mm",
                            "default": 210.0,
                            "title": "Y Dimension"
                        },
                        "description": "Printer Y dimension in mm"
                    },
                    {
                        "name": "z_dimension",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "number",
                            "description": "Printer Z dimension in mm",
                            "default": 250.0,
                            "title": "Z Dimension"
                        },
                        "description": "Printer Z dimension in mm"
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "$ref": "#/components/schemas/Body_check_model_printability_v1_printability__post"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HTTPValidationError"
                                }
