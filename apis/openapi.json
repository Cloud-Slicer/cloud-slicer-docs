                                    "profile_name": "default",
                                    "printer_config": {
                                        "bed_size_x": 210,
                                        "bed_size_y": 210,
                                        "bed_size_z": 250,
                                        "print_speed": 100,
                                        "first_layer_speed": 50,
                                        "nozzle_diameter": 0.4,
                                        "layer_height": 0.2,
                                        "perimeters": 3,
                                        "top_solid_layers": 3,
                                        "bottom_solid_layers": 3,
                                        "fill_density": 20,
                                        "support_material": false,
                                        "filament_type": "PLA",
                                        "temperature": 210,
                                        "bed_temperature": 60
                                    },
                                    "quote_config": {
                                        "currency": "USD",
                                        "cost_per_hour": 2.5,
                                        "cost_per_gram": 0.02,
                                        "base_price": 5.0
